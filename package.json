  },
  "homepage": "https://github.com/isogon/prefetch",
  "peerDependencies": {
    "react": "0.14.x || 15.x.x",
    "react-redux": "~4.x.x || ~5.x.x",
    "react-router": "~2.x.x || ~3.x.x"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.15.0",
